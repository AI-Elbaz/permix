{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.react.tsbuildinfo",
    "target": "ES2022",
    "jsx": "preserve",
    "jsxImportSource": "react",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src/react/*.ts", "src/react/*.tsx"]
}
